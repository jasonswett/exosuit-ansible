server {
  listen 80 default_server;
  listen 443 ssl default_server;
  server_name {{ passenger_server_name }};
  passenger_enabled on;
  passenger_app_env {{ passenger_app_env }};
  root {{ passenger_app_root }};

  # ssl_certificate /etc/nginx/app_exosuit_io.pem;
  # ssl_certificate_key /etc/nginx/app_exosuit_io.key;

  include sites-enabled/env;
}
